=link_to "Back to home", root_path, class: "btn btn-success goHome"
= form_for(resource, as: resource_name, url: registration_path(resource_name), html:{class:"form-signin"}) do |f|
  %h2 Sign up
  = devise_error_messages!
  .field
    = f.email_field :email, autofocus: true, placeholder:"Email Address", class:"form-control"
  .field
    = f.password_field :password, autocomplete: "off", placeholder:"Password", class:"form-control"
  .field
    = f.password_field :password_confirmation, autocomplete: "off", placeholder:"Password Confirmation", class:"form-control"
    %br
  .field
    = f.text_field :first_name, autocomplete: "off", placeholder:"First Name", class:"form-control"
  .field
    = f.text_field :last_name, autocomplete: "off", placeholder:"Last Name", class:"form-control"
  .field
    = f.text_field :cell_phone, autocomplete: "off", placeholder:"Cell Phone", class:"form-control"
  .field
    = f.text_field :work_phone, autocomplete: "off", placeholder:"Work Phone", class:"form-control"
  .field
    = f.text_field :street_address, autocomplete: "off", placeholder:"Street Address", class:"form-control"
  .field
    = f.text_field :street_address2, autocomplete: "off", placeholder:"Street Address 2", class:"form-control"
  .field
    = f.text_field :city, autocomplete: "off", placeholder:"City", class:"form-control"
  .field
    = f.text_field :state, autocomplete: "off", placeholder:"State", class:"form-control"
  .field
    = f.text_field :zipcode, autocomplete: "off", placeholder:"Zipcode", class:"form-control"
    %br
  .actions
    = f.submit "Register", class:"btn btn-lg btn-primary btn-block"
= render "devise/shared/links"
